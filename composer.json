{
    "name": "mrden/mkad-distance",
    "description": "Расчет расстояний за МКАД",
    "type": "library",
    "version": "4.0.0",
    "keywords": [
        "distance",
        "mkad",
        "мкад",
        "расстояние",
        "расстояние за мкад"
    ],
    "license": "MIT",
    "require": {
        "php": "^8",
        "ext-curl": "*",
        "ext-json": "*",
        "mrden/php-osrm": "^3.0",
        "psr/simple-cache": "^1.0",
        "yandex/geo": "*"
    },
    "authors": [
        {
            "name": "Denis Kiselev",
            "email": "kiselevden911@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mrden\\MkadDistance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpunit/phpunit": "^10.5",
        "vimeo/psalm": "^5.22"
    }
}
